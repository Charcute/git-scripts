#!/usr/bin/env node

var path = require('path')
  , scripts = require('../')
  , name = path.basename(process.argv[1])
  , project = scripts();


project.run(name, process.argv.slice(2), function(err) {
  if (err) {
    console.error('Failed to exec ' + name + ' hook script')
  }
  process.exit(err.code);
});

